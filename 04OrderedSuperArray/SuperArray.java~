public class SuperArray{
    
    String[] A;
    int currentSize;
    
    public SuperArray(int size){
	currentSize=0;
	A=new String[size];
    }

    public SuperArray(){
	this(10);
    }

    public String toString(){
	String printA="[";
	for(int i=0;i<currentSize;i++){
	    printA+=" "+A[i];
	}
	printA+=" ]";
	return printA;
    }

    public int size(){
	return currentSize;
    }

    public void add(int index,String e){
	if (currentSize+2==A.length){
	    resize();	    
	}
	currentSize++;
	for(int i=currentSize;i>index;i--){
	    A[i]=A[i-1];
	}
	A[index]=e;
    }

    public void add(String e){
	add(currentSize,e);
    }

    public void resize(){
	String[] B = new String[currentSize+10];
	for(int i=0;i<currentSize;i++){
	    B[i]=A[i];
	}
	A=B;
    }
	
}